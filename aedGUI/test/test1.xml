
<layout>

	<window x="20" y="20" w="300" h="300" name="win1" caption="window 1">

		<button x="10" y="10" w="140" h="30" name="button1" caption="Hello!"/>

		<button x="10" y="50" w="140" h="30" name="button2" caption="World!">
		</button>

		<listbox x="10" y="90" w="140" h="100">

			<listboxcolumn type="string" caption="column1">

				<listboxstringitem caption="item 1"/>
				<listboxstringitem caption="item 2"/>
				<listboxstringitem caption="item 3"/>

			</listboxcolumn>

		</listbox>

		<staticimage x="160" y="10" value="test.bmp"/>
		<staticimage x="160" y="100" value="test.bmp" colorkey="true" key_r="255" key_g="255" key_b="255"/>

	</window>

	<window x="350" y="100" w="225" h="350" name="win2" caption="window 2">

		<statictext x="10" y="20" w="180" h="25" caption="Username:"/>

		<textbox x="10" y="50" w="180" h="25" caption="anonymous"/>

		<frame x="10" y="100" w="180" h="120">

			<radiobuttongroup x="10" y="10" w="100" h="80">

				<radiobutton caption="String 1"/>
				<radiobutton caption="String 2"/>
				<radiobutton caption="String 3"/>

			</radiobuttongroup>

		</frame>
		
		<checkbox x="10" y="240" w="180" h="20" name="check1" caption="Checked" value="true"/>
		<checkbox x="10" y="270" w="180" h="20" name="check2" caption="Not Checked" value="false"/>

	</window>

</layout>
